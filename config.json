{
  "app": {
    "name": "AgenticAI",
    "version": "1.0.0",
    "description": "The Next Generation AI Product"
  },
  "ai": {
    "search_enabled": true,
    "confidence_threshold": 0.8,
    "max_results": 10,
    "models": ["gpt-4", "claude-3"],
    "features": {
      "natural_language_processing": true,
      "real_time_search": true,
      "message_routing": true,
      "analytics": true
    }
  },
  "api": {
    "rate_limit": 100,
    "timeout": 30,
    "cors_enabled": true
  },
  "security": {
    "encryption_enabled": true,
    "data_privacy": "high",
    "audit_logging": true
  }
}